{
    "name": "zerosonesfun/flarum-ext-sm2",
    "description": "Automatic URL to minimal inline audio player.",
    "type": "flarum-extension",
    "keywords": ["discussion", "audio"],
    "license": "MIT",
    "authors": [
        {
            "name": "Billy Wilcosky",
            "homepage": "https://www.wilcosky.com",
            "role": "Creator"
        }
    ],
    "support": {
        "issues": "https://github.com/zerosonesfun/flarum-ext-sm2/issues",
        "source": "https://github.com/zerosonesfun/flarum-ext-sm2",
        "forum": "https://discuss.flarum.org"
    },
    "homepage": "https://www.wilcosky.com",
    "funding": [
        {
            "type": "website",
            "url": "https://www.wilcosky.com"
        }
    ],
    "require": {
        "flarum/core": "^0.1.0-beta.15"
    },
    "autoload": {
        "psr-4": {
            "ZerosOnesFun\\Sm2\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Audio (SM2)",
            "category": "feature",
            "icon": {
                "name": "fas fa-volume-up",
                "backgroundColor": "#ddd",
                "color": "#000"
            }
        }
    }
}
